{
  "name": "delhi-ultimate",
  "version": "1.0.0",
  "description": "Delhi Ultimate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "NODE_ENV=production webpack -p --progress",
    "postinstall": "npm run copy",
    "copy": "npm run copy:mat-comp && npm run copy:mat-default",
    "copy:mat-comp": "cp -R node_modules/materialize-css/sass/components src/css/materialize",
    "copy:mat-default": "cp node_modules/materialize-css/sass/style.scss src/css/materialize"
  },
  "author": "Satyajeet Parida <shatyajeet@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "material-ui": "^0.15.2",
    "materialize-css": "^0.97.6",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
